        <zkclient.version>0.1</zkclient.version>
        <curator.version>2.7.0</curator.version>
        <mybatis.verion>3.2.8</mybatis.verion>
        <mybatis-spring.version>1.2.2</mybatis-spring.version>
        <spring.version>5.2.10.RELEASE</spring.version>
        <mysql.version>5.1.18</mysql.version>
        <druid.version>1.0.15</druid.version>
        <kafka.version>0.8.2.2</kafka.version>
        <commons.lang.version>2.6</commons.lang.version>
        <jedis.version>2.7.0</jedis.version>
        <lettuce.version>5.0.3.RELEASE</lettuce.version>
        <spring.session.redis>2.3.1.RELEASE</spring.session.redis>
    </properties>

    <dependencies>
        <dependency>
            <groupId>log4j</groupId>
            <artifactId>log4j</artifactId>
            <version>${log4j.version}</version>
        </dependency>
